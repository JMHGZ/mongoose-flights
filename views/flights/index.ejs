<%- include('../partials/header') %>
  <table id="list">
    <thead>
      <tr>
        <th>Flight</th>
        <th>Flight<br>Number</th>
        <th></th>
      </tr>
    </thead>
    <tbody>
      <% flights.forEach(function(f) { %>
        <tr>
          <td><%= f.airline %></td>
          <td><%= f.flightNo %></td>
          <td><a href="/flights/<%= f._id %>">DETAILS</a></td>
        </tr>
        <% }); %>
      </tbody>
    </table>
    
  <%- include('../partials/footer') %> 

